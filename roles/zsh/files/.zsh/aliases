alias ch="cd $HOME"
alias cc="cd $HOME/code"
alias cdot="cd $HOME/code/dotfiles"

alias pip="python3 -m pip install --user --upgrade"

alias l="ls -la"
alias ll="ls -la"
alias ls="ls --color=auto"
alias lsa="ls -la"
alias ldot="ls -ld .*"

alias dl="youtube-dl"

alias grep="grep --color=auto"

